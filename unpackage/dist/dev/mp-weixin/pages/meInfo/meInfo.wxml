<view class="page page-fill"><view class="page-block info-list"><view data-event-opts="{{[['tap',[['operator',['$event']]]]]}}" class="item-wrapper face-line-upbottom" bindtap="__e"><view class="info-words">头像</view><view class="right-wrapper"><image class="face" src="{{userInfo.faceIcon}}"></image><view class="arrow-block"><image class="arrow-ico" src="../../static/icos/left-gray-arrow.png"></image></view></view></view><view class="line-top"><view class="line"></view></view><view data-event-opts="{{[['tap',[['modifyNickname',['$event']]]]]}}" class="item-wrapper" bindtap="__e"><view class="info-words">昵称</view><view class="right-wrapper"><view class="gray-fields">{{''+userInfo.nickname+''}}</view><view class="arrow-block"><image class="arrow-ico" src="../../static/icos/left-gray-arrow.png"></image></view></view></view><view class="line-top"><view class="line"></view></view><view data-event-opts="{{[['tap',[['modifyCity',['$event']]]]]}}" class="item-wrapper" bindtap="__e"><view class="info-words">城市</view><view class="right-wrapper"><view class="gray-fields">{{''+userInfo.city+''}}</view><view class="arrow-block"><image class="arrow-ico" src="../../static/icos/left-gray-arrow.png"></image></view></view></view><view class="line-top"><view class="line"></view></view><view data-event-opts="{{[['tap',[['modifySex',['$event']]]]]}}" class="item-wrapper" bindtap="__e"><view class="info-words">性别</view><view class="right-wrapper"><view class="gray-fields"><block wx:if="{{userInfo.gender==0}}"><view>男</view></block><block wx:else><block wx:if="{{userInfo.gender==1}}"><view>女</view></block><block wx:else><view>未选择</view></block></block></view><view class="arrow-block"><image class="arrow-ico" src="../../static/icos/left-gray-arrow.png"></image></view></view></view><view class="line-top"><view class="line"></view></view><view data-event-opts="{{[['tap',[['modifyBirthday',['$event']]]]]}}" class="item-wrapper" bindtap="__e"><view class="info-words">生日</view><view class="right-wrapper"><view class="gray-fields">{{''+userInfo.birthday+''}}</view><view class="arrow-block"><image class="arrow-ico" src="../../static/icos/left-gray-arrow.png"></image></view></view></view><view class="line-top"><view class="line"></view></view></view><view class="footer-wrapper"><view data-event-opts="{{[['tap',[['cleanStorage',['$event']]]]]}}" class="footer-words" bindtap="__e">清理缓存</view><view data-event-opts="{{[['tap',[['logout',['$event']]]]]}}" class="footer-words" style="margin-top:10rpx;" bindtap="__e">退出登录</view></view></view>