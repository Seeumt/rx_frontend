.travels{padding: 10rpx 20rpx;}
.classify text{display: block; font-size: 27rpx; color: #14181e; background: #f7f7f7;
    padding: 10rpx 20rpx; border-radius: 20rpx;
    margin: 0 30rpx;}
.classify{display: -webkit-box;display: -webkit-flex;display: flex;}
.classify-text{font-size: 30rpx; color: #14181e; font-weight: bold;}
.classify-view{display: -webkit-box;display: -webkit-flex;display: flex; -webkit-box-pack: start; -webkit-justify-content: flex-start; justify-content: flex-start; -webkit-box-align: center; -webkit-align-items: center; align-items: center;}
/* 选中的样式 */
.activetext{background: #ffdd00 !important;}
/* 标题 */
.place-text{color: #808080 !important; font-size: 30rpx;}
.travels-title{margin: 40rpx 0;}
/* 描述 */
.travels-describe textarea{width: 100%; color: #808080 !important; font-size: 30rpx;}
/* 上传图片 */
.topimg image{width: 150rpx; height: 150rpx; border-radius: 20rpx;}
.topimg{padding-left: 20rpx; padding-top: 10rpx;}
/* 九宫格 */
.conteng{display: -webkit-box;display: -webkit-flex;display: flex; -webkit-flex-wrap: wrap; flex-wrap: wrap;
    -webkit-justify-content: space-around;
            justify-content: space-around;
    -webkit-box-align: center;
    -webkit-align-items: center;
            align-items: center;
    margin-left: 4px; margin-right: 4px;}
.conteng-img{
    width: calc((100% / 3) - 25px) !important;
    margin: 4px;
    height: 176rpx;
    position: relative;
}
.uploadimg{width: 100% !important;height: 100% !important;border-radius: 10rpx;}
/* 删除图片 */
.deleteimg{width: 38rpx; height: 38rpx;position: absolute;top: 6rpx;right: 6rpx;}
/* 视频 */
.uploadvideo video{width: 100%; height: 400rpx; border-radius: 5rpx;}
.uploadvideo{margin: 0 8px; border-radius: 5rpx; position: relative;}
/* 覆盖视频 */
.pause{width: 50rpx; height: 50rpx; position: absolute; top: 6rpx; right: 6rpx;}
/* 位置 */
.address-site{display: -webkit-box;display: -webkit-flex;display: flex; -webkit-box-align: center; -webkit-align-items: center; align-items: center;}
.address-site image{width: 40rpx; height: 40rpx; margin-right: 10rpx;}
.address-site text{font-size: 30rpx; color: #00a2ff;}
.address-text{font-size: 30rpx; color: #333333; padding-right: 40rpx;}
.address{display: -webkit-box;display: -webkit-flex;display: flex; background: #f1f1f1; padding: 20rpx 0 20rpx 20rpx; margin-top: 30rpx;
    margin-bottom: 160rpx;}
/* 发布 */
.release{background: #ffd655; width: 100%;height: 100rpx;color: #ffffff;font-weight: bold;
    font-size: 40rpx;
    line-height: 100rpx;
    text-align: center;
    position: fixed;
    bottom: 0;
    left: 0;
    right: 0;
    z-index: 999;}
/* 提示用户上传成功与否 */
.warp{position: fixed;
    left: 0;
    right: 0;
    top: 0;
    bottom: 0;
    background: rgba(0,0,0,0.6);
    z-index: 9999;}
.warp-view{width: 500rpx;
    height: 200rpx;
    background: #FFFFFF;
    margin: auto;
    position: absolute;
    -webkit-position:absolute;
    top: 0;
    left: 0;
    bottom: 0;
    right: 0;
    border-radius: 8rpx;
    overflow: hidden;
}
/* 提示框 */
.tipmin text{font-size: 34rpx;
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-box-pack: center;
    -webkit-justify-content: center;
            justify-content: center;
    line-height: 200rpx;
}

